{
  "config": {
    "target": "https://cfbackendz.herokuapp.com/",
    "phases": [
      {
        "duration": 1,
        "arrivalRate": 2
      },
      {
        "duration": 1,
        "arrivalRate": 1
      }
    ]
    ,
    "processor" : "./load-test-create-user.js"
  },
  "scenarios": [
    {
      "name" : "Create Users",
      "flow": [
        {
          "function": "create"
        },
        {
          "post": {
            "url": "welcome/signup",
            "json": {
              "username": "{{ username }}",
              "email": " {{ email }}",
              "password": "{{ password }}"
            },
            "capture" : {
              "json": "$.token",
              "as": "token"
            }
          }
        }
        ,
        {
          "get": {
            "url": "profile"
          }
        }
      ]
    }
  ]
}
