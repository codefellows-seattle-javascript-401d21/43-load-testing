{
  "config": {
    "target": "http://localhost:3000",
    "processor": "./artillery-payload.js",
    "phases": [ 
      {
        "duration": 2,
        "arrivalRate": 10
      }
     ],
     "scenarios": {
       "flow":{
         "post":{
            "url": "/landing/signup",
            "json": {
              "username": "{{ username }}" ,
              "password": "{{ password }}",
              "email": "{{ email }}"
            }
         }

       }
     }

}
}